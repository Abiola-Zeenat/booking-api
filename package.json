    "@types/supertest": "^6.0.3",
    "cross-env": "^10.0.0",
    "jest": "^30.1.3",
    "pg-mem": "^3.0.5",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
