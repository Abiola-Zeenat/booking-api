  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Abiola-Zeenat/booking-api/issues"
  },
  "homepage": "https://github.com/Abiola-Zeenat/booking-api#readme",
  "dependencies": {
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "joi": "^18.0.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
